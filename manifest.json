{
	"name": "Quora Unblocker",
	"version": "1.0.0",
	"description": "Removes the login requirement and any nagging about it",
	"homepage_url": "https://github.com/sindresorhus/quora-unblocker",
	"manifest_version": 2,
	"minimum_chrome_version": "36",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"*://quora.com/*",
		"*://www.quora.com/*"
	],
	"background": {
		"scripts": [
			"node_modules/query-string/query-string.js",
			"background.js"
		]
	},
	"content_scripts": [{
		"run_at": "document_end",
		"matches": [
			"*://quora.com/*",
			"*://www.quora.com/*"
		],
		"css": [
			"content.css"
		]
	}]
}
